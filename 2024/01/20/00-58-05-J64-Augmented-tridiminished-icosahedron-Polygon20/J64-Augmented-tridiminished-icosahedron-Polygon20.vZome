      </ViewModel>
      <content xml:space="preserve">Remove the internal triangular panel and reset the strut colors.</content>
    </page>
    <page snapshot="17" title="Faces">
      <ViewModel distance="158.99606323242188" far="317.99212646484375" near="0.3974902033805847" parallel="false" stereoAngle="0.0" width="71.5482406616211">
